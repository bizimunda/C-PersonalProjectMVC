@model PersonalProject.Models.Registration

@{
    ViewBag.Title = "Register";
}

<h2>Register</h2>
@using (Html.BeginForm())
{

    <p>
        User Name: @Html.TextBoxFor(x => x.UserName)
    </p>

    <p>
        Password: @Html.TextBoxFor(x => x.Password)
    </p>

    <p>
        Frist Name: @Html.TextBoxFor(x=>x.FirstName)
    </p>

    <p>
        Last Name: @Html.TextBoxFor(x => x.LastName)
    </p>
    
    <p>
        Email: @Html.TextBoxFor(x => x.Email)
    </p>

    <p>
        Adress: @Html.TextBoxFor(x => x.Adress)
    </p>

    <p>
        Phone: @Html.TextBoxFor(x => x.Phone)
    </p>

    <p>
        Phone1: @Html.TextBoxFor(x => x.Phone1)
    </p>
    
    <input type="submit" value="Valid" />
    <form action="~/Views/Home/CompleteForm.cshtml" method="post"></form>
    <div>@Html.ValidationSummary()</div>
}

